{
	"name": "DSA - ActionKit Templates",
	"image": "mcr.microsoft.com/devcontainers/python:1-3.12-bullseye",
	"postCreateCommand": "pip install .",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"tamasfe.even-better-toml"
			],
			"settings": {
				"python.defaultInterpreterPath": ".venv/bin/python"
			}
		}
	}
}